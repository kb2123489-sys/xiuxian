{
    "build": {
        "command": "npm run build"
    },
    "assets": {
        "directory": "dist"
    },
    "routes": [
        {
            "src": "^/(assets|public)/.*$",
            "headers": {
                "Cache-Control": "max-age=31536000"
            }
        },
        {
            "src": ".*",
            "dest": "/index.html"
        }
    ]
}